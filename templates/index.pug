extends ./layout.pug
include ./mixins.pug

block title
  title Photo-Gallery

block body
  - var isLoggedIn = true

    if routeOf == 'pictures'
      h1#header.headerClass(style={color: '#513894'}) #{headline}

      h3 #{listType}
      p#name #{userName}
      div#gallery
        ul
          each val, index in collection
            li #{index} + ': title:' + #{val.title} + 'url: ' + #{val.url}
